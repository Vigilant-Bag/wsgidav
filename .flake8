[flake8]
max-line-length = 99

# Ignore some errors that may be created by Black:
# E203 whitespace before ':'
# E501 line too long
# W503 line break before binary operator
ignore = E203, E501, W503

# Options for the flake8-quotes extension:
inline-quotes = "
multiline-quotes = """
docstring-quotes = """
